function setup() {
  const graph = createCanvas(640, 480); // create canvas
  graph.parent('graph');  // then allocate to parent div

  background(125);
  fill(255);
}

function draw() {
  textSize(16);
  text(<%= Keystroke.calc_avg_all %>, 10, 90);
  const avgPrev = <%= Keystroke.calc_avg_prev_array %>;
  for (let i = 0; i < avgPrev.length; i++) {
    text(avgPrev[i], 10, 90 + 30 * (i + 1));
  }
}

